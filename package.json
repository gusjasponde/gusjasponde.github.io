{
  "name": "gusjasponde.github.io",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --hot --inline --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "deploy": "gh-pages -d build",
    "test": "nyc mocha src/**/*.test.js src/**/**/*.test.js --require @babel/register --timeout=3000",
    "coverage": "nyc --reporter=html --reporter=text npm test",
    "cy:ci": "start-server-and-test start http://localhost:3000 cy:record",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "cy:record": "./node_modules/.bin/cypress run --record",
    "eslint": "eslint --format 'node_modules/eslint-friendly-formatter' src/ cypress/ webpack.common.js webpack.dev.js webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gusjasponde/gusjasponde.github.io.git"
  },
  "author": "gusjasponde",
